  jstor_formatteddate = {Jan., 1990},
  language            = {English},
  owner               = {zjutangxin},
  publisher           = {The Econometric Society},
  timestamp           = {2013.01.10},
  url                 = {http://www.jstor.org/stable/2938333},
}

